---
const { slug } = Astro.params;
let response = await fetch(`http://localhost:8080/wp-json/wp/v2/posts?_embed&acf_format=standard&slug=${slug}`);
if (!response.ok) {
	throw new Error(`HTTP error! status: ${response.status}`);
}
let posts = await response.json();

export async function getStaticPaths() {
	let response = await fetch(`http://localhost:8080/wp-json/wp/v2/posts`);
	if (!response.ok) {
		throw new Error(`HTTP error! status: ${response.status}`);
	}
	let posts = await response.json();

	return posts.map(post => ({
		params: { slug: post.slug },
		props: { post: post },
	}));
}
